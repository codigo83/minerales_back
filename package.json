  },
  "homepage": "https://github.com/codigo83/minerales_back#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.13",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
